{
  "name": "message-bird",
  "version": "0.0.1",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^4.1",
    "phpunit/phpunit": "^7"
  },
  "config": {
    "bin-dir": "./bin",
    "optimize-autoloader": true,
    "discard-changes": true
  },
  "require": {
    "messagebird/php-rest-api": "^1.9",
    "php-amqplib/php-amqplib": "^2.7"
  }
}
